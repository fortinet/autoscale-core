{
    "name": "autoscale-core",
    "description": "Fortinet Autoscale project(s), core module.",
    "version": "3.2.0",
    "main": "dist/index.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "pre-push": "npm-run-all lint-check audit-production test build",
        "audit-production": "npm audit --production",
        "bundle": "webpack",
        "compile": "tsc --traceResolution > out.log",
        "clear-dist": "rimraf dist",
        "build": "npm run clear-dist && npm run compile",
        "make-dist": "node dist/scripts/make-dist.js",
        "build-artifacts": "npm-run-all build make-dist",
        "test": "mocha --timeout 3600000 -r ts-node/register test/{*,**/**/*}.spec.ts",
        "lint-check": "node node_modules/ftnt-devops-ci/bin/ftnt-devops-ci.js c -fl  -F .eslintignore -L .eslintignore \"{*,**/*}{.js,.json,.ts}\"",
        "lint-fix": "node node_modules/ftnt-devops-ci/bin/ftnt-devops-ci.js f -fl  -F .eslintignore -L .eslintignore \"**/*{.js,.json,.ts}\""
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-check",
            "pre-push": "npm run pre-push"
        }
    },
    "keywords": [],
    "author": "Fortinet <github@fortinet.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/mocha": "^8.2.1",
        "@typescript-eslint/eslint-plugin": "^2.24.0",
        "@typescript-eslint/parser": "^2.24.0",
        "archiver": "^4.0.1",
        "comment-json": "^3.0.2",
        "decompress": "^4.2.1",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.1",
        "eslint-plugin-mocha": "^6.3.0",
        "eslint-plugin-prettier": "^3.1.2",
        "file-loader": "^6.2.0",
        "ftnt-devops-ci": "github:fortinet/ftnt-devops-ci",
        "husky": "^4.3.0",
        "json-loader": "^0.5.7",
        "mocha": "^8.3.0",
        "mocha-loader": "^5.1.5",
        "npm-run-all": "^4.1.5",
        "prettier": "^1.19.1",
        "shelljs": "^0.8.4",
        "sinon": "^9.0.1",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.7.0",
        "typescript": "^3.9.9",
        "webpack": "^5.20.2",
        "webpack-cli": "^4.5.0"
    },
    "dependencies": {
        "@azure/arm-compute": "^15.0.0",
        "@azure/arm-network": "^23.2.0",
        "@azure/cosmos": "^3.9.5",
        "@azure/functions": "^1.2.3",
        "@azure/identity": "^1.2.2",
        "@azure/keyvault-secrets": "^4.1.0",
        "@azure/ms-rest-js": "^2.2.1",
        "@azure/ms-rest-nodeauth": "^3.0.6",
        "@azure/storage-blob": "^12.4.1",
        "@types/archiver": "^3.1.0",
        "@types/aws-lambda": "^8.10.46",
        "@types/comment-json": "^1.1.1",
        "@types/decompress": "^4.2.3",
        "@types/node": "^13.9.2",
        "@types/shelljs": "^0.8.7",
        "@types/sinon": "^9.0.1",
        "@types/xml2js": "^0.4.5",
        "aws-sdk": "^2.677.0",
        "axios": "^0.21.1",
        "chalk": "^4.0.0",
        "express": "^4.17.1",
        "http-status-codes": "^1.4.0",
        "rimraf": "^3.0.2",
        "xml2js": "^0.4.23"
    },
    "peerDependencies": {
        "archiver": "^4.0.1",
        "chalk": "^4.0.0",
        "comment-json": "^3.0.2",
        "decompress": "^4.2.1",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.4",
        "sinon": "^9.0.1",
        "typescript": "^3.9.9"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/fortinet/autoscale-core.git"
    },
    "bugs": {
        "url": "https://github.com/fortinet/autoscale-core/issues"
    },
    "homepage": "https://github.com/fortinet/autoscale-core#readme"
}
